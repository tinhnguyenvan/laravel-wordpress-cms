{
    "name": "tinhnguyenvan/laravel-cms",
    "type": "project",
    "description": "CMS Laravel sample wordpress",
    "keywords": [
        "tinhnguyenvan",
        "tinhphp",
        "framework",
        "laravel cms",
        "laravel cms wordpress",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-gettext": "*",
        "ext-json": "*",
        "astrotomic/laravel-translatable": "^11.9",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "haruncpi/laravel-id-generator": "^1.0",
        "intervention/image": "^2.5",
        "irazasyed/telegram-bot-sdk": "^2.0",
        "kreait/laravel-firebase": "^1.0",
        "laracademy/generators": "^2.1",
        "laravel/framework": "^7.1",
        "laravel/socialite": "^4.3",
        "maatwebsite/excel": "^3.1",
        "simplesoftwareio/simple-qrcode": "~2",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/zalo": "^4.1",
        "spatie/laravel-permission": "^3.7",
        "spatie/laravel-pjax": "^2.1",
        "tinhphp/woocommerce": "dev-master",
        "willvincent/laravel-rateable": "^2.2"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "mpociot/laravel-apidoc-generator": "^4.8",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [

            ],
            "aliases": {

            },
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "config/helpers.php",
            "app/Helpers/simple_html_dom.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*/*"
        }
    ]
}
